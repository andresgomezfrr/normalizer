{
  "inputs":{
    "topic1":["stream1"]
  },
  "streams":{
    "stream1":{
        "funcs":[
              {
                "name":"jqFlatMapper",
                "className":"io.wizzie.ks.normalizer.funcs.impl.JqFlatMapper",
                "properties": { 
                  "jqQuery": "{ids:[.ids|split(\",\")[]|tonumber|.+100],name}"
                }
              }
        ],
        "sinks":[
            {"topic":"output", "partitionBy":"name"}
        ]
    }
  }
}